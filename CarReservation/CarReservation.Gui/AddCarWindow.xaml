        <TextBox x:Name="TextBox_YearOfProduction" HorizontalAlignment="Left" Height="23" Margin="124,75,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <TextBox x:Name="TextBox_NumberPlate" HorizontalAlignment="Left" Height="23" Margin="124,106,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="160"/>
        <Button x:Name="Button_Cancel" Content="Fortryd" HorizontalAlignment="Left" Margin="55,141,0,0" VerticalAlignment="Top" Width="75" Click="Button_Cancel_Click"/>
        <Button x:Name="Button_AddCar0" Content="Tilføj Ny Bil" HorizontalAlignment="Left" Margin="165,141,0,0" VerticalAlignment="Top" Width="75" Click="Button_AddCar0_Click"/>

    </Grid>
</Window>
